{
  "name": "clerc",
  "version": "0.27.1",
  "author": "Ray <nn_201312@163.com> (https://github.com/so1ve)",
  "description": "Clerc: The full-featured cli framework.",
  "keywords": [
    "cli",
    "clerc",
    "arguments",
    "argv",
    "args",
    "terminal"
  ],
  "type": "module",
  "homepage": "https://github.com/so1ve/clerc/tree/main/packages/clerc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/clerc.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/so1ve/clerc/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "puild --minify",
    "watch": "puild --watch"
  },
  "devDependencies": {
    "@clerc/core": "workspace:*",
    "@clerc/plugin-completions": "workspace:*",
    "@clerc/plugin-friendly-error": "workspace:*",
    "@clerc/plugin-help": "workspace:*",
    "@clerc/plugin-not-found": "workspace:*",
    "@clerc/plugin-strict-flags": "workspace:*",
    "@clerc/plugin-version": "workspace:*"
  }
}
